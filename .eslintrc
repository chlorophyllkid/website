{
  "extends": "airbnb-base",

  "env": {
    "browser": true
  },

  "rules": {
    "no-return-assign": 0,
    "linebreak-style": 0,
    "class-methods-use-this": 0,
    "no-use-before-define": [
      "error", {
        "functions": false
      }
    ],
    "import/no-extraneous-dependencies": [
      "error",
      { "devDependencies": true }
    ]
  }
}