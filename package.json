{
  "name": "website",
  "version": "0.0.1",
  "author": "Christopher Voigt <c.voigt@queo-group.com>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/chlorophyllkid/website"
  },
  "scripts": {
    "build": "run-s build:11ty build:postcss",
    "build:11ty": "npx @11ty/eleventy",
    "build:postcss": "postcss src/css/*.css -o dist/css/index.css",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "watch:11ty": "npx @11ty/eleventy --serve",
    "watch:postcss": "postcss src/css/*.css -o dist/css/index.css --watch",
    "start": "run-p watch:11ty watch:postcss",
    "debug": "DEBUG=* npx @11ty/eleventy"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-navigation": "^0.3.3",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.12",
    "luxon": "^2.5.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "npm-run-all": "^4.1.5",
    "open-props": "^1.4.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^14.1.0",
    "postcss-jit-props": "^1.0.7",
    "postcss-nesting": "^10.1.10"
  }
}